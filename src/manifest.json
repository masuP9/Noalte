{
  "manifest_version": 2,
  "name": "Noalte",
  "description": "alt text editor for note",
  "version": "0.3.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://note.mu/*",
        "https://note.com/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "background": { "scripts": ["hot-reload.js"] }
}
